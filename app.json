{
  "expo": {
    "name": "Phinmon",
    "slug": "phinmon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#7A5FFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#7A5FFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sinaayo.phinmon"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2f796c6d-970f-49b5-9125-b8ae658de178"
      }
    },
    "plugins": ["expo-asset"]
  }
}
